{
  "name": "@ohtomi/react-whiteboard",
  "version": "0.1.1",
  "description": "A whiteboard React component using SVG",
  "author": "Kenichi Ohtomi",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s lint flow compile test",
    "clear": "rimraf dist coverage .nyc_output",
    "flow": "flow",
    "lint": "eslint src/ test/",
    "compile": "cross-env NODE_ENV=production webpack --mode production --progress --profile",
    "test": "cross-env NODE_ENV=test ava --verbose",
    "cover": "nyc --reporter=html --reporter=text npm run test",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "storybook": "start-storybook -p 9080",
    "watch": "run-p watch:*",
    "watch:src": "chokidar src/**/*.js -c \"npm run compile\"",
    "watch:test": "chokidar test/**/*.js -c \"cross-env NODE_ENV=test ava --tap {path} | tap-notify\""
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/react": "^3.2.3",
    "ava": "^0.25.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-security": "^1.2.0",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.1.5",
    "jsdom": "^11.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^12.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.1",
    "sinon": "^6.1.3",
    "tap-notify": "^1.0.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ],
    "require": [],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohtomi/react-whiteboard.git"
  },
  "keywords": [
    "react",
    "svg",
    "whiteboard"
  ],
  "bugs": {
    "url": "https://github.com/ohtomi/react-whiteboard/issues"
  },
  "homepage": "https://github.com/ohtomi/react-whiteboard"
}
